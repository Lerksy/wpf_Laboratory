<Window x:Class="wpfLab.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfLab"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" VerticalAlignment="Top">
    <WrapPanel Margin="10" Width="600">
        <Label Width="100" Margin="3">Label</Label>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <Label Width="100" Margin="3">Label</Label>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <TextBox Width="50">TextBox</TextBox>
        <Button Width="200" Margin="150, 20">Button</Button>
    </WrapPanel>

</Window>
